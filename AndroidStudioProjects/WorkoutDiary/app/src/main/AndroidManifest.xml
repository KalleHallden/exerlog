<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kalle.workoutdiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAUL" />
            </intent-filter>
        </activity>
        <activity android:name=".DiaryActivity" />
        <activity android:name=".CopyOfWorkoutActivity" />
        <activity android:name=".StatsActivity" />
        <activity android:name=".BodyStatActivity" />
        <activity android:name=".StatDiaryActivity" />
        <activity android:name=".StatChooserActivity" />
        <activity android:name=".BodyGraphActivity" />
        <activity android:name=".LayoutAnimationActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BottomNaviClass"></activity>
    </application>

</manifest>